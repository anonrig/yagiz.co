{
  "root": true,
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals"
  ],
  "plugins": [
    "simple-import-sort"
  ],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "jsx-quotes": ["error", "prefer-single"],
    "react/no-unescaped-entities": "off"
  }
}
